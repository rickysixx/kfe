def add(x y)
    x + y;

def sub(x y)
    x - y;

def mul(x y)
    x * y;

def divide(x y)
    x / y;

def lt(x y)
    x < y;

def le(x y)
    x <= y;

def gt(x y)
    x > y;

def ge(x y)
    x >= y;

def eq(x y)
    x == y;

def ne(x y)
    x != y;

def neg(x)
    -x;

def testneg()
    -3+2;

def testneg2()
    5+-9--1;

def compound()
    3 + 2 : 5 + 1 : -9;